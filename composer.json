    "require-dev": {
        "phpunit/phpunit": "^9",
        "ottaviano/faker-gravatar": "^0.2.0",
        "mockery/mockery": "^1.5"
    },
    "require": {
        "spatie/url": "^2.1",
        "allure-framework/allure-phpunit": "^2",
        "lcobucci/jwt": "^4.2",
        "guzzlehttp/guzzle": "^7.5",
